@using CarPartsShoppingList.Infrastructure.Data.Models
@model List<TransmisionViewModel>

@{
    ViewData["Title"] = "Двигател";
}

    <h4>Engine</h4>
    <hr/>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>TransmisionId</th>
            <th>TransmisionName</th>
            <th>TransmisionPrice</th>
            <th>TransmisionCode</th>
        </tr>
        @foreach (TransmisionViewModel engine in Model)
        {
            <tr>
                <td>@engine.TransmisionName</td>
                <td>@engine.TransmisionPrice</td>
                <td>@engine.TransmisionCode</td>
            </tr>
        }
    </table>